{
  "name": "@pcllab/plugin-audio-response",
  "version": "0.0.1",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "unpkg": "dist/index.global.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "buildPackage": "tsup src/index.ts --format esm,cjs --dts",
    "buildBrowser": "tsup src/index.ts --format iife --minify",
    "build": "pnpm buildPackage && pnpm buildBrowser",
    "dev": "pnpm buildPackage --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pcllab/plugins.git",
    "directory": "packages/plugin-audio-response"
  },
  "bugs": {
    "url": "https://github.com/@pcllab/plugins/issues"
  },
  "homepage": "https://github.com/@pcllab/plugins/tree/main/packages/plugin-audio-response#readme",
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "tsconfig": "workspace:*",
    "tsup": "^6.5.0"
  },
  "dependencies": {
    "@pcllab/plugin-audio-response": "^0.0.1",
    "jspsych": "^7.3.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0"
  }
}
